{
  "name": "ascii2mathml",
  "version": "0.2.3",
  "description": "The second AsciiMath – MathML converter",
  "repository": {
    "type": "git",
    "url": "https://github.com/runarberg/ascii2mathml"
  },
  "main": "index.js",
  "bin": {
    "ascii2mathml": "bin/index.js"
  },
  "scripts": {
    "prebuild": "rm -fr lib; mkdir lib",
    "build": "babel src/ -d lib/; babel < index.es6.js > index.js",
    "dist": "browserify ./ -s ascii2mathml > dist/ascii2mathml.js",
    "prepublish": "npm run build",
    "preversion": "npm run dist; git add -f dist/ascii2mathml.js",
    "postversion": "git checkout gh-pages; git checkout master dist/ascii2mathml.js ; git commit add dist/ascii2mathml; git commit -m 'version update'; git checkout master; git rm dist/ascii2mathml",
    "pretest": "npm run build",
    "test": "mocha",
    "posttest": "npm run dist"
  },
  "keywords": [
    "mathml",
    "asciimath",
    "asciimathml"
  ],
  "author": "Rúnar Berg Baugsson Sigríðarson",
  "license": "MIT",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "engines": {
    "iojs": "^1.4.3"
  },
  "devDependencies": {
    "babel": "^5.0.6",
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "expect.js": "^0.3.1",
    "mocha": "^2.1.0"
  },
  "dependencies": {
    "minimist": "^1.1.0"
  }
}
